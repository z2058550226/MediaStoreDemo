<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bybutter.mediatest">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="false"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PermissionActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BucketListActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".apitest.AppSpecificFileActivity" />
        <activity android:name=".apitest.MediaStoreActivity" />
        <activity android:name=".apitest.SafActivity" />
        <activity android:name=".ImageListActivity" />
        <activity android:name=".VideoListActivity" />
        <activity android:name=".ImageDisplayActivity" />
        <activity android:name=".PickPictureActivity" />
    </application>

</manifest>